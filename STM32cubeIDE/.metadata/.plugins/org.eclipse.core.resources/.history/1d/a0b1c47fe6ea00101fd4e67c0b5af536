#include "led.h"

#define GPIOBEN		(1U << 1) // 0b 0000 0000 0000 0000 0000 0000 0000 0010

void led_init(void){

	/* Enable clock access to led port (port B)*/
	RCC->AHB2ENR = (RCC->AHB2ENR) | GPIOBEN;
	/* Set led pin as output*/

}

void led_on(void){

	/* Set led pin HIGH (PB5)*/
}

void led_off(void){

	/* Set led pin LOW (PB5)*/
}
